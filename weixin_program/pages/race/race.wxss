.week_day {
  font-size: 16px;
  padding: 5px 0px;
  background: #fff;
  color: #323232;
  box-shadow: 0px 3px 5px #dcdcdc;
  position: fixed;
  top: 0px;
  width: 100%;
  z-index: 999;
}

.week_day .active {
  color: #ee8c14;
}

.section {
  background: #eee;
  padding: 3px 0px;
  color: #b8b8b8;
  font-size: 16px;
}

.common {
  margin-top: 40px;
}

.common_list {
  padding: 10px 0px;
  border-bottom: 1px solid #e6e6e6;
}

.list {
  font-size: 16px;
  padding: 0px 10px 10px;
  color: #9b9b9b;
}

.league_wid {
  width: 30%;
}

.league_wid text {
  display: inline-block;
  width: 40%;
  text-align: center;
}

.clash {
  width: 70%;
}

.league_wid .league {
  width:55%;
  color: #fff;
  padding: 1px 0px;
  background: #ccc;
  border-radius: 3px;
}

#panel {
  width: 95%;
  margin: 0 auto;
  background: #fff;
  color: #323232;
  font-size: 14px;
}

#panel .table_top rich-text {
  border-bottom: none;
}

#panel rich-text {
  line-height: 6vh;
  border: 1rpx solid #5dacff;
  border-right: none;
  height: 51px;
}

#panel rich-text:nth-of-type(5n) {
  border-right: 1rpx solid #5dacff;
}

#panel rich-text view {
  line-height: 18px;
}

#panel rich-text view:nth-of-type(1) {
  color: #fb6168;
  text-align: center;
}

#panel rich-text view {
  margin: 5px 0px;
}

.flex-row {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.flex-column {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: stretch;
}

.flex-cell {
  flex: 1;
}

.nothing {
  color: #9b9b9b;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  align-content: center;
  font-size: 12px;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: 0 auto;
  z-index: 1;
}

.nothing image {
  width: 55px;
  height: 72px;
  margin: 0px 0px 12px 0px;
}
