.flex-row {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.flex-column {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: stretch;
}
.container {
  padding: 0rpx 0;
  font-size: 14px;
}

.ai {
  width: 100%;
  display: flex;
  vertical-align: middle;
}

.time {
  margin: 5px 0px;
  padding: 5px 5px;
  background: #e6e6e6;
}

.ai image {
  width: 45px;
  height: 45px;
}

.team {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin: 2px 0px 2px;
}

.team view {
  width: 33%;
  height: 60px;
  text-align: center;
}

.team view image {
  width: 30px;
  height: 30px;
  margin: 0 auto;
}

.table {
  border: 1px solid #e6e6e6;
  width: 100%;
  justify-content: space-between;
  display: flex;
}

.left {
  width: 30%;
  height: 60px;
  text-align: center;
  border-right: 1px solid #e6e6e6;
  line-height: 60px;
}

.right {
  height: 60px;
  /* flex: 4; */
  float: left;
  width: 70%;
}

.right view {
  width: 24.6%;
  float: left;
  text-align: center;
  line-height: 30px;
  border-right: 0.5px solid #e6e6e6;
}

.right view.btm {
  border-bottom: 1px solid #e6e6e6;
}

.pay {
  width: 100%;
  position: fixed;
  bottom: 0px;
  background: #ccc;
}

.section {
  height: 60px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  color: #9b9b9b;
}

.heightauto {
  height: auto !important;
  display: block;
}

.star {
  padding: 0px 8rpx;
  border: 1px solid #f12b48;
  color: #f12b48;
  border-radius: 3px;
  font-size: 10px;
}

.pay .price {
  width: 50%;
  float: left;
  height: 48px;
  line-height: 48px;
  text-align: center;
}

.pay button {
  height: 48px;
  background: #bc1d13;
  border-radius: 0px;
  color: #fff;
}

.bifen {
  /* line-height: 60px; */
  font-size: 14px;
  color: #323232;
  font-weight: normal;
}

.detail {
  width: 90%;
  margin: 0 auto;
}

.bjtu {
  background-size: 100%;
  height: 160rpx;
  text-align: center;
  padding: 20px 0px;
  color: #9b9b9b;
}

.suo image {
  width: 40px;
  height: 40px;
}

.red {
  background: #e03618;
  color: #fff;
}

.correct {
  color: #bc1d13;
  text-align: center;
}

.correct {
  vertical-align: middle;
  margin: 0.2rem 0rem;
}

.no_correct {
  text-align: center;
  margin: 0.2rem 0rem;
}

.correct text {
  display: inline-block;
  margin: 0rem 0.5rem;
}

.correct image {
  width: 1rem;
  height: 1rem;
  vertical-align: middle;
}

.hide {
  display: none;
}

.tuijian {
  color: #9b9b9b;
}
.time_tui{
  margin: 10px 0px 2px;
  color: #323232;
}
