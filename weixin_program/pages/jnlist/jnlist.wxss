.container {
  padding: 0rpx 0;
}

.lunbo {
  width: 100%;
}

.swiper {
  width: 100%;
}
.slide-image {
  width: 100%;
  height: 150px;
}

.flex-wrp {
  /* height: 100px; */
  display: flex;
}
.list_item{
 width: 90%;
 margin: 0 auto;
}
.ai {
  padding:12px 0px;
  font-size: 14px;
  
  border-bottom: 0.1px solid #eee;
}

.ai .ai_logo {
  height: 45px;
}

.ai .ai_logo image {
  width: 45px;
  height: 45px;
  float: left;
}

.ai .ai_name {
  float: left;
  margin-left: 10px;
}

.ai .vs text {
  margin: 0px 10px;
  font-size: 12px;
}

.ai .pay_see {
  border: 1px solid #bc1d13;
  line-height: normal;
  height: 20px;
  padding: 0px 10px;
  border-radius: 2px;
  /* margin-top: 10px; */
  color: #bc1d13;
}

.pay_over {
  margin-top: 10px;
}

.ai .over {
  color: #9b9b9b;
  border-color: #9b9b9b;
}

.ai .success {
  background: #e03618;
  color: #fff;
  border: none;
}

.ai .lose {
  background: #9b9b9b;
  color: #fff;
  border: none;
}

.ai .over text {
  /* color: #cecece; */
  font-size: 12px;
}

.logo {
  width: 40px;
  height: 40px;
  vertical-align: middle;
  line-height: 40px;
  margin-right: 5px;
}

.team {
  line-height: 26px;
  background: #e6e6e6;
  margin: 10px 0px 0px;
  color: #9b9b9b;
}

.star {
  padding: 0px 2px;
  /* background: #BC1D13; */
  border: 1px solid #f12b48;
  color: #f12b48;
  border-radius: 3px;
  font-size: 12px;
  letter-spacing: 1px;
}
.star2 {
   padding: 4px 6px;
  font-size: 14px;
  /* font-weight: bold; */
}
.rate {
  padding: 0rpx 0px;
  border: 1px solid #f12b48;
  border-radius: 3px;
  color: #f12b48;
  font-size: 14px;
  height: 20px;
  line-height: 20px;
}



.team .flex-item {
  padding: 0rpx 10rpx;
}

.team .flex-item {
  float: right;
}

.AIName {
  color: #323232;
  margin: 2px 0px;
}

/* 页面排版修改部分 */

.vs_status {
  font-size: 16px;
  color: #323232;
}

.number {
  font-size: 13px;
  color: #9a9a9a;
}

.number .week {
  color: #419bff;
  margin-right: 10px;
}

.flex-row {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 16px;
}

.flex-row text {
  margin-right: 5px;
  margin-left: 5px;
  color: #333;
}

.distance {
  margin-top: 5px;
}
.distance text{
  width: 45%;
}
.distance text:nth-of-type(1){
  text-align: right;
}
.distance text:nth-of-type(2){
  text-align: center;
  width: 10%;
}
.distance text:nth-of-type(3){
  text-align: left;
}
.nothing {
  color: #9b9b9b;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  align-content: center;
  font-size: 12px;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: 0 auto;
  z-index: 1;
}

.nothing image {
  width: 55px;
  height: 72px;
  margin: 0px 0px 12px 0px;
}

